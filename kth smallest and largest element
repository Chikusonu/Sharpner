let a=[2,3,4,1,5]
function max_k(a,k){

    let sorted =a.sort((a,b)=>(b-a))
    return a[k-1]
}
function min_k(a,k){

    let sorted =a.sort((a,b)=>(a-b))
    return a[k-1]
}
console.log(" second largest element in arrray is "+max_k(a,2))
console.log(" second  smallest  element in arrray is "+min_k(a,2))
